{
  "name": "along",
  "version": "1.0.0",
  "scripts": {
    "build": "next build",
    "dev": "concurrently \"npm run server:next\" \"npm run server:rx\"",
    "deploy": "now -e EXPRESS_PORT=\"3001\" -e NEXT_PORT=\"1313\" -e REDIS_HOST=@redis-host -e REDIS_PASS=@redis-pass -e REDIS_PORT=@redis-port",
    "server:next": "node server.js",
    "server:rx": "node rx-server.js",
    "start": "NODE_ENV=production concurrently \"npm run server:rx\" \"npm run server:next\""
  },
  "dependencies": {
    "babel-plugin-emotion": "^8.0.4",
    "body-parser": "^1.18.2",
    "bufferutil": "^3.0.3",
    "emotion": "^8.0.5",
    "emotion-server": "^8.0.5",
    "express": "^4.16.3",
    "http": "^0.0.0",
    "next": "^5.0.0",
    "now": "^10.2.1",
    "prop-types": "^15.6.1",
    "pubnub": "^4.20.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-emotion": "^8.0.5",
    "redis": "^2.8.0",
    "redis-server": "^1.2.0",
    "rxjs": "^5.5.7",
    "url": "^0.11.0",
    "utf-8-validate": "^4.0.0",
    "ws": "^5.0.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "dotenv": "^5.0.1",
    "fs": "^0.0.1-security"
  },
